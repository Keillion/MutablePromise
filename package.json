{
  "name": "mutable-promise",
  "version": "1.1.13",
  "description": "Wrapper for Promise. Resolvable, rejectable, redirectable.",
  "files": [
    "/dist",
    "/src"
  ],
  "main": "./dist/index.js",
  "~~module": "./dist/index.mjs",
  "module": "./dist/index.esm.js",
